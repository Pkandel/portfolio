{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "This is a boilerplate for react-redux application.",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development npm-run-all --parallel run:server watch:css",
    "lint": "node_modules/.bin/esw webpack.config.babel.js src",
    "fix": "eslint --fix src/.; exit 0;",
    "run:server": "webpack-dev-server",
    "lint:watch": "npm run lint --watch",
    "watch:css": "node-sass src/ -o src/ --watch --recursive",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf dist",
    "build:html": "babel-node tools/buildHtml.js",
    "preprod": "npm-run-all clean-dist build:html",
    "prod": " babel-node tools/build.js",
    "postprod": "NODE_ENV=production babel-node tools/prodServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pkandel/strategic-frontend"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "babel"
  ],
  "author": "Prakash Kandel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pkandel/strategic-frontend/issues"
  },
  "homepage": "https://github.com/pkandel/strategic-frontend#readme",
  "dependencies": {
    "antd": "^3.22.1",
    "axios": "^0.19.0",
    "compression": "^1.7.4",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/register": "^7.5.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.3.3",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.2.2",
    "eslint-config-standard": "^14.0.1",
    "eslint-config-standard-react": "^9.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-watch": "^6.0.0",
    "expect": "^24.9.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.1.1",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.12.11",
    "redux-immutable-state-invariant": "^2.1.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  }
}
